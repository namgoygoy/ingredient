<?xml version="1.0" encoding="utf-8"?>
<!-- 선택된 아이템 배경 제거 (투명) -->
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 선택된 상태: 투명 배경 -->
    <item android:state_checked="true">
        <shape android:shape="rectangle">
            <solid android:color="@android:color/transparent" />
        </shape>
    </item>
    <!-- 기본 상태: 투명 배경 -->
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@android:color/transparent" />
        </shape>
    </item>
</selector>

